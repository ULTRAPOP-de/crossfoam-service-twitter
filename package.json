{
  "name": "@crossfoam/service-twitter",
  "version": "1.1.9",
  "description": "Twitter Service module",
  "main": "dst/index.js",
  "scripts": {
    "lint": "tslint -p tslint.json --force",
    "build": "tsc",
    "typescript": "npm run lint && npm run build",
    "watch": "nodemon "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultrapop-de/crossfoam-service-twitter.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ultrapop-de/crossfoam-service-twitter/issues"
  },
  "homepage": "https://github.com/ultrapop-de/crossfoam-service-twitter#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "dst/*"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run typescript",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/core-js": "^2.5.3",
    "@types/node": "^14.0.14",
    "nodemon": "^1.19.4",
    "tslint": "^5.20.1",
    "typescript": "^3.9.6",
    "web-ext-types": "^3.1.0"
  },
  "dependencies": {
    "@crossfoam/data": "^1.0.3",
    "@crossfoam/ui-helpers": "^1.1.0",
    "twitter-lite": "git://github.com/ultrapop-de/twitter-lite.git"
  }
}
